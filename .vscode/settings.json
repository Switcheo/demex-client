{
  "editor.formatOnSave": true,
  "eslint.validate": ["javascript", "typescript"],
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true
  },
  "html.format.enable": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/build": true,
    "**/logs": true,
    "**/dist": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "python.formatting.provider": "none"
}
